{
  "manifest_version": 2,

  "name": "1-Click Timetracking",
  "description": "Lightweight time tracking extension.",
  "version": "1.0",

  "permissions": [
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
	"default_icon": {                    
	  "19": "img/icon_19.png",           
	  "38": "img/icon_38.png"            
	}
  }
}